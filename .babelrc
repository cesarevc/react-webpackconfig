{
    "presets": [
        "@babel/preset-env", 
        [
			"@babel/preset-react",
			{
				"targets": {
					"esmodules": true
				}
			}
		],
		[
			"minify",
			{
				"builtIns": false,
				"evaluate": false,
				"mangle": false
			}
		]
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties"
    ]
}